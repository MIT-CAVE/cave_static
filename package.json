    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.4",
    "@mui/x-data-grid": "^5.17.2",
    "@reduxjs/toolkit": "^1.8.5",
    "crypto-js": "^4.1.1",
    "d3-array": "^3.2.0",
    "d3-color": "^3.1.0",
    "d3-scale": "^4.0.2",
    "echarts": "^5.3.3",
    "echarts-for-react": "^3.0.2",
    "expr-eval": "^2.0.2",
    "isomorphic-fetch": "^3.0.0",
