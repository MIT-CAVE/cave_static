    "@emotion/eslint-plugin": "^11.10.0",
    "babel-loader": "^8.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-ramda": "^2.5.1",
    "lint-staged": "^12.5.0",
    "prettier": "2.7.1",
    "simple-git-hooks": "^2.8.0"
