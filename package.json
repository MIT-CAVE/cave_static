{
  "name": "cave_static",
  "version": "1.0.0",
  "scripts": {
    "build": "export $(cat .env | xargs) && react-scripts build",
    "lint": "prettier --cache --write . && eslint --cache --report-unused-disable-directives --ext .js,.jsx --max-warnings 0 --fix .",
    "reset": "npx rimraf ./package-lock.json && npm run setup",
