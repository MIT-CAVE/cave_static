BUILD_VERSION="1.0.0-dev"
PUBLIC_URL='https://builds.mitcave.com/$BUILD_VERSION'
S3_BUCKET="builds-mitcave"
GENERATE_SOURCEMAP=false

# Settings
# Attempts to connect to React Devtools on port 8097 if true 
## Note: Can only be used on localhost
USE_DEVTOOLS='false'
# Use Redux Devtools if true
## Note: Can be used on localhost
## NOTE: Included in a build if `dev` is in the build version otherwise it is false
USE_REDUX_DEVTOOLS='true'

# Auto generated settings
BUILD_PATH="build/$BUILD_VERSION"
REACT_APP_BUILD_VERSION=$BUILD_VERSION
REACT_APP_USE_DEVTOOLS=$USE_DEVTOOLS
REACT_APP_USE_REDUX_DEVTOOLS=$USE_REDUX_DEVTOOLS
